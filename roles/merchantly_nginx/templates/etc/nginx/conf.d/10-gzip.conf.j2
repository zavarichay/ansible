# {{ ansible_managed }}

gzip on;
gzip_comp_level 6;
gzip_http_version 1.0;
gzip_proxied any;
gzip_disable "MSIE [1-6]\.";
gzip_types application/atom+xml application/json application/x-javascript application/javascript application/xml application/xml+rss text/comma-separated-values text/css text/javascript  text/plain text/xml image/svg+xml;
